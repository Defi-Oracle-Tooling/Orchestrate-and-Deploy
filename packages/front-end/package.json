    "dependencies": {
        "axios": "^1.2.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^14.3.1",
        "@types/node": "^18.15.11",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.10",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitest/coverage-v8": "^3.1.1",
        "eslint": "^8.34.0",
        "happy-dom": "^17.4.4",
        "jsdom": "^26.0.0",
        "ts-loader": "^9.4.2",
        "typescript": "^4.9.5",
        "vitest": "^0.34.0",
        "webpack": "^5.76.0",
        "webpack-cli": "^5.0.2",
        "webpack-dev-server": "^4.11.1"
    }
