{
    "name": "front-end",
    "version": "1.0.0",
    "main": "dist/index.js",
    "scripts": {
        "build": "webpack --config webpack.config.js",
        "start": "webpack serve --config webpack.config.js --mode development --open",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint ./src --ext .ts,.tsx",
        "doc": "pnpm typedoc --out docs ./src"
